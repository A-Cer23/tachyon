{
    "$id": "account/register/request",
    "requiresLogin": false,
    "type": "object",
    "properties": {
        "messageId": {
            "type": "string"
        },
        "commandId": {
            "const": "account/register/request",
            "type": "string"
        },
        "data": {
            "examples": [
                {
                    "email": "bob@test.com",
                    "username": "bob",
                    "password": "plsnerfconsuls"
                }
            ],
            "type": "object",
            "properties": {
                "email": {
                    "format": "email",
                    "examples": [
                        "bob@test.com"
                    ],
                    "type": "string"
                },
                "username": {
                    "minLength": 3,
                    "maxLength": 20,
                    "pattern": "^[A-Za-z0-9_-]+$",
                    "examples": [
                        "Bob"
                    ],
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
            },
            "required": [
                "email",
                "username",
                "password"
            ]
        }
    },
    "required": [
        "messageId",
        "commandId",
        "data"
    ]
}