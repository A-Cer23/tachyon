{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "matchmaking/queue/request",
    "title": "MatchmakingQueueRequest",
    "scopes": [
        "tachyon.lobby"
    ],
    "type": "object",
    "properties": {
        "type": {
            "const": "request",
            "type": "string"
        },
        "messageId": {
            "type": "string"
        },
        "commandId": {
            "const": "matchmaking/queue",
            "type": "string"
        },
        "data": {
            "type": "object",
            "properties": {
                "queues": {
                    "minItems": 1,
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "queues"
            ],
            "title": "MatchmakingQueueRequestData"
        }
    },
    "required": [
        "type",
        "messageId",
        "commandId",
        "data"
    ]
}