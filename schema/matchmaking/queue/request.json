{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://schema.beyondallreason.dev/tachyon/matchmaking/queue/request.json",
    "title": "MatchmakingQueueRequest",
    "tachyon": {
        "source": "user",
        "target": "server",
        "scopes": [
            "tachyon.lobby"
        ]
    },
    "type": "object",
    "properties": {
        "type": {
            "const": "request",
            "type": "string"
        },
        "messageId": {
            "type": "string"
        },
        "commandId": {
            "const": "matchmaking/queue",
            "type": "string"
        },
        "data": {
            "type": "object",
            "properties": {
                "queues": {
                    "minItems": 1,
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "queues"
            ],
            "title": "MatchmakingQueueRequestData"
        }
    },
    "required": [
        "type",
        "messageId",
        "commandId",
        "data"
    ]
}