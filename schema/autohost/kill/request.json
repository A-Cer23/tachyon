{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://schema.beyondallreason.dev/tachyon/autohost/kill/request.json",
    "title": "AutohostKillRequest",
    "scopes": [
        "tachyon.lobby"
    ],
    "type": "object",
    "properties": {
        "type": {
            "const": "request",
            "type": "string"
        },
        "messageId": {
            "type": "string"
        },
        "commandId": {
            "const": "autohost/kill",
            "type": "string"
        },
        "data": {
            "type": "object",
            "properties": {
                "battleId": {
                    "format": "uuid",
                    "type": "string"
                }
            },
            "required": [
                "battleId"
            ],
            "title": "AutohostKillRequestData"
        }
    },
    "required": [
        "type",
        "messageId",
        "commandId",
        "data"
    ]
}