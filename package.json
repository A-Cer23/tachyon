{
  "name": "tachyon-protocol",
  "version": "1.5.5",
  "description": "",
  "scripts": {
    "dev": "nodemon --watch src/** --ext ts --ignore src/meta.ts --exec npm run build",
    "build": "npx tsx src/index.ts",
    "tidy": "npm run lint && npm run format",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "prepublishOnly": "patch-package && npm run build && npm run test",
    "postpublish": "git commit -a -m \"updated tachyonVersion const literal\" && git push"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "require": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "./validators": {
      "default": {
        "default": "./dist/validators.mjs",
        "types": "./dist/validators.d.mts"
      },
      "import": {
        "default": "./dist/validators.mjs",
        "types": "./dist/validators.d.mts"
      },
      "require": {
        "default": "./dist/validators.js",
        "types": "./dist/validators.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyond-all-reason/tachyon.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beyond-all-reason/tachyon/issues"
  },
  "homepage": "https://github.com/beyond-all-reason/tachyon#readme",
  "dependencies": {
    "ajv": "^8.12.0",
    "jaz-ts-utils": "^7.10.0",
    "type-fest": "^4.14.0"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.32.9",
    "@swc/core": "^1.3.65",
    "@swc/helpers": "^0.5.1",
    "@swc/wasm": "^1.3.65",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.43.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.5.0",
    "json-schema-faker": "^0.5.6",
    "json-schema-to-typescript": "^13.0.2",
    "nodemon": "^2.0.22",
    "patch-package": "^7.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "overrides": {
    "@sinclair/typebox": "$@sinclair/typebox",
    "@typescript-eslint/eslint-plugin": "$@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser": "$@typescript-eslint/parser",
    "eslint-plugin-unused-imports": "$eslint-plugin-unused-imports",
    "eslint-plugin-simple-import-sort": "$eslint-plugin-simple-import-sort"
  }
}
